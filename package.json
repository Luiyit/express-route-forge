{
  "name": "express-route-forge",
  "version": "0.2.9",
  "description": "Express JS router builder",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn removeDist && rollup -c",
    "buildPublish": "yarn build && yarn publish",
    "publish": "npm publish",
    "addUser": "npm adduser",
    "removeDist": "rimraf dist",
    "cleanCache": "yarn cache clean && npm cache clean --force",
    "watch": "npm run build -- --watch src"
  },
  "keywords": [
    "express",
    "node",
    "typescript",
    "mongodb",
    "sentry"
  ],
  "author": "Luiyit Hernandez",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@swc/core": "^1.10.0",
    "@types/express": "^5.0.0",
    "rimraf": "^6.0.1",
    "tslib": "^2.8.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "express": "^4.21.2",
    "firebase-admin": "^13.0.2",
    "joi": "^17.13.3"
  }
}
